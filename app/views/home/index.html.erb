<div class="container mx-auto px-4 my-4">
  <h1 class="text-2xl font-bold text-center my-4">Welcome
    <%= current_user&.email || "Guest" %>!</h1>
  <!-- app/views/home/index.html.erb -->
  <div class="container mx-auto px-4 py-6">
    <h1 class="text-3xl font-bold text-center mb-6">Dashboard</h1>
    <!-- Stats Section -->
    <div class="grid grid-cols-1 sm:grid-cols-3 gap-4 text-center mb-6">
      <div class="p-4 bg-gray-100 border border-gray-300 rounded-lg">
        <p class="text-2xl font-semibold text-gray-800">100</p>
        <p class="text-sm text-gray-600">Total Entries</p>
      </div>
      <div class="p-4 bg-gray-100 border border-gray-300 rounded-lg">
        <p class="text-2xl font-semibold text-gray-800">50</p>
        <p class="text-sm text-gray-600">Active Entries</p>
      </div>
      <div class="p-4 bg-gray-100 border border-gray-300 rounded-lg">
        <p class="text-2xl font-semibold text-gray-800">75%</p>
        <p class="text-sm text-gray-600">Completion Rate</p>
      </div>
    </div>
    <!-- CRUD Table -->
    <div class="overflow-x-auto relative shadow-md sm:rounded-lg mb-6">
      <table
        class="
          w-full text-sm text-left text-gray-500 p-4 bg-gray-100 border border-gray-300
          rounded-lg
        "
      >
        <thead class="text-xs text-gray-700 uppercase bg-gray-50">
          <tr>
            <th scope="col" class="py-3 px-6">Name</th>
            <th scope="col" class="py-3 px-6">Date</th>
            <th scope="col" class="py-3 px-6">Status</th>
            <th scope="col" class="py-3 px-6">Actions</th>
          </tr>
        </thead>
        <tbody>
          <tr class="bg-white border-b hover:bg-gray-50">
            <td class="py-4 px-6">Sample Item 1</td>
            <td class="py-4 px-6">01/01/2024</td>
            <td class="py-4 px-6">Active</td>
            <td class="py-4 px-6">
              <button class="font-medium text-blue-600 hover:underline mr-2">Edit</button>
              <button class="font-medium text-red-600 hover:underline">Delete</button>
            </td>
          </tr>
          <!-- Repeat for other entries -->
        </tbody>
      </table>
    </div>
    <!-- Action Buttons -->
    <div
      class="
        flex justify-end gap-4 p-4 bg-gray-100 border border-gray-300 rounded-lg
      "
    >
      <button class=" bg-green-500 hover:bg-green-700 font-bold py-2 px-4 rounded ">
        Add New
      </button>
      <button class=" bg-blue-500 hover:bg-blue-700 font-bold py-2 px-4 rounded ">
        Refresh
      </button>
    </div>
  </div>

</div>

